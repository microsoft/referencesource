    <Style x:Uid="Style_9" TargetType="{x:Type swdt:ToolboxControl}">
        <Style.Resources>
            <swdt:ToolboxItemImageConverter x:Uid="swdt:ToolboxItemImageConverter_1" x:Key="ImageConverter"/>
