  <!-- toggle button style, used to mimic TreeView expand/collapse button behaviour -->
  <Style x:Uid="Style_26" TargetType="{x:Type ToggleButton}" x:Key="expandCollapseButtonStyle">
    <Setter x:Uid="Setter_162" Property="AutomationProperties.AutomationId" Value="expandCollapseToggleButton"/>
    <Setter x:Uid="Setter_163" Property="Template">
      <Setter.Value>
        <ControlTemplate x:Uid="ControlTemplate_5" TargetType="{x:Type ToggleButton}" >
          <Border x:Uid="Border_1" Width="16" Height="16" Background="Transparent" Padding="5,5,5,5">
            <Path x:Uid="ExpandPath" x:Name="ExpandPath" Fill="Transparent" Stroke="#FF989898">
              <Path.Data>
                <PathGeometry x:Uid="PathGeometry_1">
                  <PathGeometry.Figures>
                    <PathFigureCollection x:Uid="PathFigureCollection_1" >
                      <PathFigure x:Uid="PathFigure_1" IsFilled="True" StartPoint="0 0" IsClosed="True">
                        <PathFigure.Segments>
                          <PathSegmentCollection x:Uid="PathSegmentCollection_1" >
                            <LineSegment x:Uid="LineSegment_1" Point="0 6"/>
                            <LineSegment x:Uid="LineSegment_2" Point="6 0"/>
                          </PathSegmentCollection>
                        </PathFigure.Segments>
                      </PathFigure>
                    </PathFigureCollection>
                  </PathGeometry.Figures>
                </PathGeometry>
              </Path.Data>
              <Path.RenderTransform>
                <RotateTransform x:Uid="RotateTransform_1" Angle="135" CenterX="3" CenterY="3"/>
              </Path.RenderTransform>
            </Path>
          </Border>

          <ControlTemplate.Triggers>
            <Trigger x:Uid="Trigger_13" Property="IsChecked" Value="True">
              <Setter x:Uid="Setter_164" TargetName="ExpandPath" Property="RenderTransform">
                <Setter.Value>
                  <RotateTransform x:Uid="RotateTransform_2" Angle="180" CenterX="3" CenterY="3"/>
                </Setter.Value>
              </Setter>
              <Setter x:Uid="Setter_165" Property="Fill" TargetName="ExpandPath" Value="Black"/>
              <Setter x:Uid="Setter_166" Property="Stroke" TargetName="ExpandPath" Value="Black"/>
            </Trigger>

            <MultiTrigger x:Uid="MultiTrigger_1" >
              <MultiTrigger.Conditions>
                <Condition x:Uid="Condition_1" Property="IsMouseOver" Value="True"/>
                <Condition x:Uid="Condition_2" Property="IsChecked" Value="False"/>
              </MultiTrigger.Conditions>
              <Setter x:Uid="Setter_167" Property="Stroke" TargetName="ExpandPath" Value="Black"/>
            </MultiTrigger>

          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>

  <!-- default auto-wrapping style for tool-tips -->
  <Style x:Uid="Style_28" x:Key="{x:Type ToolTip}" TargetType="{x:Type ToolTip}">
    <Setter x:Uid="Setter_187" Property="ContentTemplate">
        <Setter.Value>
          <DataTemplate x:Uid="DataTemplate_2">
            <ContentPresenter x:Uid="ContentPresenter_3" Content="{TemplateBinding ToolTip.Content}">
              <ContentPresenter.Resources>
