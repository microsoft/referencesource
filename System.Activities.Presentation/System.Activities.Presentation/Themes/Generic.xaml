                            <ColumnDefinition x:Uid="ColumnDefinition_2" Width="Auto"/>
                            <ColumnDefinition x:Uid="ColumnDefinition_3" Width="*" SharedSizeGroup="toolLabel"/>
                        </Grid.ColumnDefinitions>
                        <Image x:Uid="Image_1" Grid.Column="0" Source="{Binding Path=Bitmap, Converter={StaticResource ImageConverter}}"/>
                        <TextBlock x:Uid="TextBlock_2" Grid.Column="1" Text="{Binding Path=DisplayName}" Margin="5,1,5,0"/>                
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Setter x:Uid="Setter_57" Property="CategoryItemStyle">
            <Setter.Value>
                <Style x:Uid="Style_10" TargetType="{x:Type TreeViewItem}">
                    <Setter x:Uid="Setter_129" Property="AutomationProperties.Name" Value="{Binding Path=CategoryName}"/>
                    <Setter x:Uid="Setter_58" Property="IsExpanded" Value="True"/>
                    <Setter x:Uid="Setter_59" Property="Background" Value="{StaticResource WorkflowViewElementBackground}"/>
                    <Setter x:Uid="Setter_60" Property="FontWeight" Value="Bold"/>
                    <Setter x:Uid="Setter_61" Property="BorderThickness" Value="1"/>
                    <Setter x:Uid="Setter_62" Property="BorderBrush" Value="{x:Null}"/>
                    <Style.Triggers>
