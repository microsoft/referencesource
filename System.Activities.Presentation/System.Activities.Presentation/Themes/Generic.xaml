        <Setter x:Uid="Setter_50" Property="Template" Value="{StaticResource ToolboxDefaultTemplate}"/>
        <Setter x:Uid="Setter_51" Property="Background" Value="{StaticResource DesignerViewBackground}"  />
        <Setter x:Uid="Setter_52" Property="BorderBrush" Value="{StaticResource WorkflowViewElementBorderBrush}"  />
        <Setter x:Uid="Setter_53" Property="BorderThickness" Value="1"/>
        <Setter x:Uid="Setter_54" Property="Grid.IsSharedSizeScope" Value="True"/>
        <Setter x:Uid="Setter_55" Property="CategoryTemplate">
            <Setter.Value>
                <HierarchicalDataTemplate x:Uid="HierarchicalDataTemplate_1" DataType="{x:Type swdt:ToolboxCategory}" ItemsSource="{Binding Path=Tools}" >
                    <Grid x:Uid="Grid_1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition x:Uid="ColumnDefinition_1" Width="*" SharedSizeGroup="categoryLabel"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock x:Uid="TextBlock_1" Grid.Column="0" Text="{Binding Path=CategoryName}" MinWidth="150" Margin="5,3"/>
                    </Grid>
                </HierarchicalDataTemplate>
            </Setter.Value>
        </Setter>
        <Setter x:Uid="Setter_56" Property="ToolTemplate">
            <Setter.Value>
                <DataTemplate x:Uid="DataTemplate_1" DataType="{x:Type swdt:ToolboxItemWrapper}">
                    <Grid x:Uid="Grid_2">
